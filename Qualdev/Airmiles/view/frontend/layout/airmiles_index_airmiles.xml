<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../../../lib/internal/Magento/Framework/View/Layout/etc/page_configuration.xsd"> 
<update handle="customer_account"/> 
<body> 
	<attribute name="class" value="custom-account"/>
    <referenceBlock name="page.main.title"> 
            <action method="setPageTitle"> 
                <argument translate="true" name="title" xsi:type="string">Airmiles Points</argument> 
            </action> 
     </referenceBlock> 
     <referenceContainer name="content"> 
        <block class="Qualdev\Airmiles\Block\Airmiles" name="airmiles" template="Qualdev_Airmiles::airmiles.phtml" cacheable="false" > 
        </block> 
    </referenceContainer> 
</body> 
</page> 
